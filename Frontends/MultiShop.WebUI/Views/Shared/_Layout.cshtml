<!DOCTYPE html>
<html lang="en">

 @await Component.InvokeAsync("LayoutHead")

<body>
	<!-- Topbar Start -->
	@await Component.InvokeAsync("LayoutTopbar")
	<!-- Topbar End -->

	<!-- Navbar Start -->
	@await Component.InvokeAsync("LayoutNavbar")
	<!-- Navbar End -->

	<div class="main-content">
		@RenderBody()
	</div>

	<!-- Footer Start -->
	@await Component.InvokeAsync("LayoutFooter")
	<!-- Footer End -->

	<!-- Scripts Start -->
	@await Html.PartialAsync("_ScriptsPartial")
	@await RenderSectionAsync("Scripts", required: false)
	<!-- Scripts End -->
</body>

</html>